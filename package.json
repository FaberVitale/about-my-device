{
  "name": "about-my-device",
  "description": "A Web app that displays a collection of useful infos about the device",
  "private": "true",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "cross-env NODE_ENV=development sapper dev",
    "build": "cross-env NODE_ENV=production npx --no-install sapper build",
    "export": "cross-env NODE_ENV=production npx --no-install sapper export",
    "start": "node __sapper__/build",
    "prettify": "prettier --write",
    "lint:ts": "tsc --noEmit",
    "validate": "svelte-check --ignore src/node_modules/@sapper"
  },
  "browserslist": {
    "production": [
      ">1%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "compression": "^1.7.1",
    "cross-env": "^7.0.2",
    "polka": "next",
    "sirv": "^1.0.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@tsconfig/svelte": "^1.0.10",
    "@types/compression": "^1.7.0",
    "@types/lodash-es": "^4.17.3",
    "@types/node": "^14.11.1",
    "@types/polka": "^0.5.1",
    "autoprefixer": "^10.0.4",
    "file-loader": "^6.0.0",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.2",
    "postcss": "^8.1.10",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^13.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.1",
    "sapper": "^0.28.0",
    "sass": "^1.29.0",
    "svelte": "3.29.0",
    "svelte-check": "^1.0.46",
    "svelte-loader": "^2.9.0",
    "svelte-preprocess": "^4.5.2",
    "ts-loader": "^8.0.4",
    "tslib": "^2.0.1",
    "typescript": "^4.0.3",
    "webpack": "^4.7.0",
    "webpack-modules": "^1.0.0"
  }
}
